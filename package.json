{
  "name": "@imikailoby/fr24-csv-parser",
  "version": "1.0.0",
  "description": "A CSV parser for files exported from myFlightRadar24, returning structured flight data",
  "author": "Mikhail Prosmitskiy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imikailoby/fr24-csv-parser.git"
  },
  "main": "dist/index.min.js",
  "types": "dist/index.min.d.js",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "format": "prettier . --write --log-level warn"
  },
  "keywords": [
    "csv",
    "parse",
    "typescript",
    "flight",
    "radar",
    "myFlightRadar24",
    "my",
    "flightradar"
  ],
  "license": "MIT",
  "engines": {
    "node": "20.15.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rollup": "^4.29.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "type": "module"
}
